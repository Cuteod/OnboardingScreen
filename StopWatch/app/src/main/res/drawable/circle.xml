<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval">

    <solid
        android:color="@android:color/transparent" />
    <stroke
        android:width="2dp"
        android:color="@color/colourAsh"
        />
    <padding
        android:bottom="50dp"
        android:left="10dp"
        android:right="10dp"
        android:top="50dp"
        />
    <corners
        android:radius="200dp"
        />
    <size
        android:width="100dp"
        android:height="120dp"
        />


</shape>